# F5 Distributed Cloud Configuration
# Copy this file to .env and update with your values

# Required: F5xc tenant URL
F5XC_TENANT_URL=https://your-tenant.console.ves.volterra.io

# Required: F5xc API access token
F5XC_ACCESS_TOKEN=your-access-token-here

# Collection Intervals (seconds)
F5XC_QUOTA_INTERVAL=600      # Quota/usage/billing data (10 minutes)
F5XC_CDN_INTERVAL=120        # CDN load balancer metrics (2 minutes)
F5XC_HTTP_LB_INTERVAL=120    # HTTP load balancer stats (2 minutes)
F5XC_TCP_LB_INTERVAL=120     # TCP load balancer stats (2 minutes)
F5XC_UDP_LB_INTERVAL=120     # UDP load balancer stats (2 minutes)
F5XC_DNS_HEALTH_INTERVAL=60  # DNS health checks (1 minute)
F5XC_SECURITY_INTERVAL=180   # Security events (3 minutes)
F5XC_BOT_DEFENSE_INTERVAL=300 # Bot defense metrics (5 minutes)
F5XC_SYNTHETIC_INTERVAL=120  # Synthetic monitoring (2 minutes)

# Optional: HTTP server port (default: 8080)
F5XC_EXP_HTTP_PORT=8080

# Optional: Log level (default: INFO)
F5XC_EXP_LOG_LEVEL=INFO